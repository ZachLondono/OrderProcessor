<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:simpleType name="materialCore">
    <xs:restriction base="xs:string">
      <xs:enumeration value="match" />
      <xs:enumeration value="pb" />
      <xs:enumeration value="ply" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="materialFinishType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="mela" />
      <xs:enumeration value="veneer" />
      <xs:enumeration value="paint" />
      <xs:enumeration value="none" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="paintedSideType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="None" />
      <xs:enumeration value="Top" />
      <xs:enumeration value="Bottom" />
      <xs:enumeration value="Left" />
      <xs:enumeration value="Right" />
      <xs:enumeration value="Interior" />
      <xs:enumeration value="Exterior" />
      <xs:enumeration value="BothSides" />
      <xs:enumeration value="Custom" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="drillingType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="finished left" />
      <xs:enumeration value="finished right" />
      <xs:enumeration value="drilled through" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="productionNotes">
    <xs:sequence>
      <xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="cabinet">
    <xs:sequence>
      <xs:element name="qty" type="xs:unsignedByte" />
      <xs:element name="width" type="xs:decimal" />
      <xs:element name="height" type="xs:decimal" />
      <xs:element name="depth" type="xs:decimal" />
      <xs:element name="leftSide" type="xs:string" />
      <xs:element name="rightSide" type="xs:string" />
      <xs:element name="assembled" type="xs:string" />
      <xs:element name="unitPrice" type="xs:string" />
      <xs:element name="room" type="xs:string" />
      <xs:element name="comment" type="xs:string" />
      <xs:element name="boxMaterial">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="core" type="materialCore" />
            <xs:element name="type" type="materialFinishType" />
            <xs:element name="finish" type="xs:string" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="finishMaterial">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="core" type="materialCore" />
            <xs:element name="type" type="materialFinishType" />
            <xs:element name="finish" type="xs:string" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="fronts">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="type" type="xs:string" />
            <xs:element name="style" type="xs:string" />
            <xs:element name="color" type="xs:string" />
            <xs:element name="finish_type" type="xs:string" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="edgeBandColor" type="xs:string" />
      <xs:element name="productionNotes" type="productionNotes" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <xs:element name="order">
    <xs:complexType>
      <xs:sequence>
        
        <xs:element name="number" type="xs:unsignedShort" />
        <xs:element name="name" type="xs:string" />
        <xs:element name="orderDate" />
        <xs:element name="note" type="xs:string" />
        <xs:element name="description" type="xs:string" />
        
        <xs:element name="customer">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute name="id" type="xs:unsignedByte" use="required" />
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        
        <xs:element name="shipping">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="method" type="xs:string" />
              <xs:element name="date" type="xs:string" />
              <xs:element name="tax" type="xs:decimal" />
              <xs:element name="instructions" type="xs:string" />
              <xs:element name="attention" type="xs:string" />
              <xs:element name="address">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="line1" />
                    <xs:element name="line2" />
                    <xs:element name="line3" />
                    <xs:element name="city" />
                    <xs:element name="state" />
                    <xs:element name="zip" />
                    <xs:element name="country" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        
        <xs:element name="invoice">
